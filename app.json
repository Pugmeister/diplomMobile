{
  "expo": {
    "name": "SpaMobileV2",
    "slug": "SpaMobileV2",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./app/images/spa-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#3CB371"
    },
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/images/spa-logo.png",
        "backgroundColor": "#3CB371"
      }
    },
    "web": {
      "favicon": "./app/images/spa-logo.png"
    }
  }
}
